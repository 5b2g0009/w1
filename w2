private void Button_OK_Click(object sender, RoutedEventArgs e)
    {
        //MessageBox. Show("Hello, WPF!", "Greeting");
        int n = Convert.ToInt32(TextBox_number.Text);
        //MessageBox.Show($"You entered: (n}", "Input");
        string result = "";
        for (int i = 1; i <= n; i++)
        {
            for （int j = 1; j <= n; j++）
    {
                result += $"(i)*(j)=(i*j)\t";
            }
            result += "\n";
        }
        TestBox_result.Text = result;
    }
}
